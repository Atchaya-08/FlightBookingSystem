CREATE TABLE FLIGHTBOOK_TB (
RECORDID VARCHAR2(12) PRIMARY KEY,
PASSENGERNAME VARCHAR2(40) NOT NULL,
FLIGHTNUMBER VARCHAR2(20) NOT NULL,
TRAVEL_DATE DATE NOT NULL,
SEATNO VARCHAR2(10),
TICKETNO VARCHAR2(20),
REMARKS VARCHAR2(100)
);
CREATE SEQUENCE FLIGHTBOOK_SEQ
START WITH 10
MAXVALUE 99
INCREMENT BY 1;
